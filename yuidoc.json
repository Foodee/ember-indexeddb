{
  "name": "ember-indexeddb",
  "description": "A utility for using IndexedDB with ember & ember-data",
  "version": "0.1.1",
  "indexModule": "Usage",
  "url": "https://github.com/mydea/ember-indexeddb",
  "options": {
    "paths": [
      "addon",
      "addon-test-support"
    ],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "themedir" : "node_modules/yuidoc-ember-theme",
    "helpers" : ["node_modules/yuidoc-ember-theme/helpers/helpers.js"],
    "enabledEnvironments": ["development", "production"]
  }
}
